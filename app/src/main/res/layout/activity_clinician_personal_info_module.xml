            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="8dp">

                <com.google.android.material.textview.MaterialTextView
                    android:id="@+id/BMITv"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
