<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:paddingLeft="4dp"
    android:paddingRight="4dp"
    android:paddingBottom="45dp">

    <view
        android:id="@+id/card_personal"
        class="com.google.android.material.card.MaterialCardView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="4dp"
        android:layout_marginRight="4dp"
        android:outlineAmbientShadowColor="@color/indigo"
        android:outlineProvider="background"
        android:outlineSpotShadowColor="@color/indigo"
        app:cardBackgroundColor="@color/accent_layout"
        app:cardCornerRadius="12dp"
        app:cardElevation="8dp"
        app:cardPreventCornerOverlap="true"
        app:cardUseCompatPadding="true">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:padding="8dp">

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <com.google.android.material.textview.MaterialTextView
                    android:id="@+id/tvpersonal"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:background="@drawable/border_bottom"
                    android:drawableLeft="@drawable/ic_person"
                    android:drawableTint="@color/active_nav_btn"
                    android:fontFamily="@font/dinpro"
                    android:gravity="center_vertical"
                    android:text="Personal Information"
                    android:textColor="@color/primary_Text"
                    android:textSize="13sp"
                    android:textStyle="bold" />
            </TableRow>

            <TextView
                android:id="@+id/fullNameTv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                android:fontFamily="@font/dinpro"
                android:gravity="center_vertical"
                android:text="Full Name"
                android:textColor="@color/primary_Text"
                android:textSize="12sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/tv_clinicianFullName"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="4dp"
                android:background="@drawable/bg_profile_info"
                android:fontFamily="@font/helvetica"
                android:maxLines="3"
                android:textColor="@color/information_text"
                android:textSize="12sp" />

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="8dp">

                <com.google.android.material.textview.MaterialTextView
                    android:id="@+id/idNumTv"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="2dp"
                    android:layout_weight="1"
                    android:fontFamily="@font/dinpro"
                    android:gravity="center_vertical"
                    android:text="ID Number"
                    android:textColor="@color/primary_Text"
                    android:textSize="12sp"
                    android:textStyle="bold" />

                <com.google.android.material.textview.MaterialTextView
                    android:id="@+id/gradeSectionTv"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:fontFamily="@font/dinpro"
                    android:gravity="center_vertical"
                    android:text="Grade &amp; Section"
                    android:textColor="@color/primary_Text"
                    android:textSize="12sp"
                    android:textStyle="bold" />
            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="4dp">

                <com.google.android.material.textview.MaterialTextView
                    android:id="@+id/tv_clinicianIdNum"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="2dp"
                    android:layout_weight="1"
                    android:background="@drawable/bg_profile_info"
                    android:fontFamily="@font/helvetica"
                    android:paddingLeft="2dp"
                    android:textColor="@color/information_text"
                    android:textSize="12sp" />

                <TextView
                    android:id="@+id/tv_clinicianGradeSection"
